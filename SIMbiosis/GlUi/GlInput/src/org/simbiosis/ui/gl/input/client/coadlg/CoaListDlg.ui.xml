<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:t='urn:import:org.simbiosis.ui.gl.input.client.editor'>

	<g:HTMLPanel>
		<g:DialogBox ui:field="mainPanel" animationEnabled="true"
			modal="true" glassEnabled="true">
			<g:caption>
				<b>Daftar Coa</b>
			</g:caption>
			<g:HTMLPanel width="410px" height="330px">
				<table width="100%">
					<tr>
						<td>
							<t:CoaListTable ui:field='resultTable' width="400px" height="300px"/>
						</td>
					</tr>
					<tr>
						<td>
							<g:HorizontalPanel>
								<g:Button ui:field="cancel">Batal</g:Button>
								<g:Button ui:field="ok">Pilih</g:Button>
							</g:HorizontalPanel>
						</td>
					</tr>
				</table>
			</g:HTMLPanel>
		</g:DialogBox>
	</g:HTMLPanel>
</ui:UiBinder>
